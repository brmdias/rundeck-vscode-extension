{
  "name": "rundeck-vscode-extension",
  "displayName": "Rundeck VSCode Extension",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "https://github.com/brmdias/rundeck-vscode-extension",
  "activationEvents": [
    "onCommand:rundeck-vscode-extension.uploadJob",
    "onCommand:rundeck-vscode-extension.connectCluster",
    "onCommand:rundeck-vscode-extension.testConnection",
    "onCommand:rundeck-vscode-extension.clearConnection",
    "onCommand:rundeck-vscode-extension.editJobScript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rundeck-vscode-extension.uploadJob",
        "title": "Upload Rundeck Job"
      },
      {
        "command": "rundeck-vscode-extension.connectCluster",
        "title": "Connect to Rundeck Cluster"
      },
      {
        "command": "rundeck-vscode-extension.testConnection",
        "title": "Test Rundeck Connection"
      },
      {
        "command": "rundeck-vscode-extension.clearConnection",
        "title": "Clear Rundeck Connection"
      },
      {
        "command": "rundeck-vscode-extension.editJobScript",
        "title": "Edit Job Script"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
